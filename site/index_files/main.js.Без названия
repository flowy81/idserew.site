(function(){'use strict';if(navigator.userAgent.match(/IEMobile\/10\.0/)){var msViewportStyle=document.createElement('style');msViewportStyle.appendChild(document.createTextNode('@-ms-viewport{width:auto!important}'));document.querySelector('head').appendChild(msViewportStyle);}})();Math.map=function(value,inMin,inMax,outMin,outMax){return(value-inMin)*(outMax-outMin)/(inMax-inMin)+outMin;};(function(window,$){$.fn.exists=function(){return this.length>0;};$.inview=function(el,factor){if(el instanceof $==false)el=$(el);factor=factor||0;var win=$(window),scrolled=win.scrollTop(),viewed=scrolled+win.height(),height=el.outerHeight(),offset=el.offset(),top=offset.top,bottom=top+height;return(top+height*factor)<=viewed&&(bottom-height*factor)>=scrolled;};$.throttle=function(fn,threshold,scope){var last,timer;threshold||(threshold=250);return function(){var context=scope||this,args=arguments,now=+new Date;if(last&&now<last+threshold){clearTimeout(timer);timer=setTimeout(function(){last=now;fn.apply(context,args);},threshold);}else{last=now;fn.apply(context,args);}};};$.debounce=function(fn,delay,initial){var timer=null;return function(){var context=this,args=arguments;clearTimeout(timer);timer=setTimeout(function(){fn.apply(context,args);},delay);initial&&fn.apply(context,args);};};})(window,jQuery);jQuery(function($){site.window=$(window);site.body=$(document.body);site.window.on('resize',function(event){event.width=site.window.width();event.height=site.window.height();site.trigger('resize',event);}).on('scroll',function(event){var scrollTop=site.window.scrollTop(),direction=(scrollTop<site.window.scrolled)?'up':'down';event.amount=scrollTop;site.window.scrolled=scrollTop;site.trigger('scroll',event);site.trigger('scroll:'+direction,event);});FastClick.attach(document.body);$.view();site.body.on('click','.dropdown-menu-static',function(event){event.stopPropagation();});if(!Modernizr.tablet&&!Modernizr.mobile){$('[data-toggle="tooltip"]').tooltip({container:'body'});}else{$('[data-toggle="tooltip"]').on('click',function(event){alert($(this).attr('title'));});}});